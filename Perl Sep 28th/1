if(/<.*\D*>/){
        print "1\n";
        if(/<!.*>/){
            print "2\n";
            ;
        }
        else{
            print "3\n";
            print "$& ";
        }
    }




if(/<[a-zA-Z]*.*\D*\w*>/){
    if(/<!--.* -->/){
        ;
    }
    else{
        #print "$_ ";
        print "$& ";
    }
}